{
    "name": "defaultvalue/dockerizer_for_php",
    "description": "Docker-based development for PHP projects with transparent Docker configurations.",
    "version": "3.2.1",
    "type": "project",
    "authors": [{
        "name": "Maksym Zaporozhets",
        "email": "maksimz@default-value.com",
        "homepage": "https://www.linkedin.com/in/maksym-zaporozhets-78890789/",
        "role": "Tech Lead"
    }],
    "homepage": "https://github.com/DefaultValue/dockerizer_for_php",
    "readme": "https://github.com/DefaultValue/dockerizer_for_php/blob/master/Readme.md",
    "require": {
        "php": "~8.0.2|~8.1|~8.2",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-posix": "*",
        "ext-simplexml": "*",
        "ext-fileinfo": "*",
        "ext-zip": "*",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/process": "^6.0",
        "composer/semver": "^3.2",
        "symfony/http-client": "^6.0",
        "monolog/monolog": "^2.7",
        "aws/aws-sdk-php": "^3.253",
        "yosymfony/toml": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "DefaultValue\\Dockerizer\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
